{
  "env": {
    "development": {
      "presets": [
        [
          "@babel/env",
          {
            "loose": true,
            "modules": false
          }
        ]
      ]
    },
    "es": {
      "presets": [
        [
          "@babel/env",
          {
            "loose": true,
            "modules": false
          }
        ]
      ]
    },
    "lib": {
      "presets": [
        [
          "@babel/env",
          {
            "loose": true
          }
        ]
      ]
    },
    "production": {
      "presets": [
        [
          "@babel/env",
          {
            "loose": true,
            "modules": false
          }
        ]
      ]
    },
    "test": {
      "presets": [
        [
          "@babel/env",
          {
            "loose": true
          }
        ],
        "@babel/react"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-syntax-class-properties"
      ]
    }
  }
}
